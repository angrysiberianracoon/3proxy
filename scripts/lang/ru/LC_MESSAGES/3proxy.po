msgid "port must be in range 1024 - 65535"
msgstr "порт должен быть в диапазоне 1024 - 65535"

#: proxy.py:264
msgid "the name must contain only letters, numbers and symbols [_ - . @]"
msgstr "имя должно состоять только из букв, чисел и символов [_ - . @]"

#: proxy.py:280
msgid "Enter port for HTTP proxy, (for ex. 3128)"
msgstr "Введите номер порта для HTTP прокси, (например 3128)"

#: proxy.py:280 proxy.py:287
msgid "or leave blank for disable"
msgstr "или оставьте пустым для отключения протокола"

#: proxy.py:287
msgid "Enter port for SOCKS 4/4.5/5 proxy, (for ex. 1080)"
msgstr "Введите номер порта для SOCKS 4/4.5/5 прокси, (например 1080)"

#: proxy.py:298
msgid "restart server"
msgstr "перезагрузка сервера"

